   "source": [
    "file = open('data1.csv','w')\n",
    "file.write('x,x_x,y')\n",
    "file.write(\"\\n\")\n",
    "import random\n",
    "for i in range(100):\n",
    "    x = random.uniform(0,100)\n",
    "    x_x = x**2\n",
    "    y = random.uniform(0,100)\n",
    "    file.write(str(x)+\",\"+str(x_x)+\",\"+str(y))\n",
    "    file.write(\"\\n\")"
   ]
